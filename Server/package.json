{
  "name": "Charity-Counter-Server",
  "version": "1.1.3",
  "description": "Displays a contdown and donations on a monitor.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "prestart": "npm install",
    "package": "electron-packager . 'Charity Counter Server' --all",
    "osx": "electron-packager . 'Charity Counter Server' --platform=darwin --arch=ia32,x64",
    "win": "electron-packager . 'Charity Counter Server' --platform=win32 --arch=ia32,x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paviro/Charity-Counter.git"
  },
  "keywords": [
    "contdown",
    "donations",
    "Charity"
  ],
  "author": "Paul-Vincent Roll",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paviro/Charity-Counter/issues"
  },
  "homepage": "https://github.com/paviro/Charity-Counter#readme",
  
  "devDependencies": {
    "electron-prebuilt": "latest",
    "electron-packager": "latest"
  },
  "dependencies": {
    "http": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "jquery": "latest",
    "chartist": "latest",
    "sweetalert2": "latest",
    "air-datepicker": "latest"
    }
}
